  "type": "module",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/p5": "^0.9.0",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
